{
  "name": "bunja",
  "type": "module",
  "version": "2.0.0",
  "description": "State Lifetime Manager",
  "main": "dist/bunja.cjs",
  "module": "dist/bunja.js",
  "types": "dist/bunja.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/bunja.d.ts",
        "default": "./dist/bunja.js"
      },
      "require": {
        "types": "./dist/bunja.d.cts",
        "default": "./dist/bunja.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./solid": {
      "import": {
        "types": "./dist/solid.d.ts",
        "default": "./dist/solid.js"
      },
      "require": {
        "types": "./dist/solid.d.cts",
        "default": "./dist/solid.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "./dist/react.d.ts"
      ],
      "solid": [
        "./dist/solid.d.ts"
      ],
      "*": [
        "./dist/bunja.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsdown"
  },
  "keywords": [
    "bunja",
    "di"
  ],
  "author": "JongChan Choi <jong@chan.moe>",
  "license": "Zlib",
  "devDependencies": {
    "@types/react": "^19",
    "react": "^19",
    "solid-js": "^1.9.7",
    "tsdown": "^0.12.7",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": ">=19",
    "solid-js": "^1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    }
  }
}
